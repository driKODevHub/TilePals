<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <ui:Template name="Todays Stats Content" src="project://database/Assets/Plugins/Tiny%20Giant%20Studio/DevTools/DevTrails/Scripts/Editor/Templates/Todays%20Stats%20Content.uxml?fileID=9197481963319205126&amp;guid=211aca66c89ada041b70cf359ea5ee0a&amp;type=3#Todays Stats Content" />
    <ui:Template name="Project Stats Content" src="project://database/Assets/Plugins/Tiny%20Giant%20Studio/DevTools/DevTrails/Scripts/Editor/Templates/Project%20Stats%20Content.uxml?fileID=9197481963319205126&amp;guid=64bc0ad57b85f744894467515023980d&amp;type=3#Project Stats Content" />
